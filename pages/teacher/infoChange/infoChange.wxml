<!--pages/teacher/infoChange/infoChange.wxml-->
<view>
    <form bindsubmit="formSubmit">
        <van-field value="{{jobTitle}}" name="jobTitle" clearable label="职称"/>
        <van-field value="{{sex}}" name="sex" clearable label="性别" />
        <van-field value="{{userMessage.tel}}" name="tel" clearable label="电话" />
        <van-field value="{{userMessage.college}}" name="college" clearable label="学院" />
        <button formType="submit">提交</button>
    </form>
</view>