/* components/teacher/paper/paper.wxss */
.con {
    width: 94%;
    margin: auto;
    box-shadow: 5px 5px 3px #f8f8f8;
}

.con .classList {
    width: 100%;
    height: 100rpx;
    border-bottom: 1px solid #f8f8f8;
}

.con .classList text {
    line-height: 100rpx;
    margin-left: 40rpx;
    letter-spacing: 2rpx;
    font-size: 1rem;
}

.con .classList .button {
    height: 60rpx;
    width: 160rpx;
    float: right;
    margin-top: 20rpx;
    margin-right: 40rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 0.8rem;
    letter-spacing: 1rpx;
    color: #fff;
    background: rgb(115, 191, 238);
}